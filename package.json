{
  "name": "rjon",
  "version": "0.4.0",
  "description": "route js object notation is a way to standardize route metadata for use and testing in  universal route interpreter software",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "ack-reload app/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm-run-all -s build:dist build:js build:index build:css",
    "build:js": "ng build prod --prod --output-hashing=all",
    "build:index": "pug app/index.pug --out app/",
    "compile:prefx": "ack-angular-fx --igniter void --select childStag,absoluteSwap,200,500 --effects fade,slide --out ./src/prefx.ts",
    "compile:templates": "npm-run-all compile:templates:src compile:templates:example",
    "compile:templates:example": "ack-pug-bundler app/src/pugs/ app/src/templates/ --outType ts --oneToOne",
    "compile:templates:src": "ack-pug-bundler src/pugs/ src/templates/ --outType ts --oneToOne",
    "compile:templates:src:watch": "npm run compile:templates:src -- --watch",
    "watch:templates": "npm-run-all compile:templates:src:watch compile:templates:example:watch",
    "compile:templates:example:watch": "npm run compile:templates:example -- --watch",
    "build:css": "ack-sass app/styles.scss app/assets/styles.css --production",
    "build:dist:styles": "ack-sass app/styles.scss dist/styles.css --production",
    "build:dist:markdown": "ack-sass app/markdown.scss dist/markdown.css --production",
    "build:dist:css": "npm-run-all build:dist:styles build:dist:markdown",
    "watch:css": "ack-sass app/styles.scss app/assets/styles.css --watch",
    "watch:js:prod:hmr": "ng serve prod --configuration=hmr --open --port 4202",
    "watch": "npm-run-all --parallel watch:templates watch:js:prod:hmr watch:css",
    "build:dist": "ngc --declaration --project src"
  },
  "keywords": [
    "route",
    "javascript",
    "object",
    "notation",
    "rjon",
    "router",
    "routes",
    "route-map"
  ],
  "author": "Acker Apple",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AckerApple/rjon.git"
  },
  "bugs": {
    "url": "https://github.com/AckerApple/rjon/issues"
  },
  "homepage": "https://github.com/AckerApple/rjon#readme",
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.1",
    "@angular/animations": "^7.2.0",
    "@angular/common": "^7.2.0",
    "@angular/compiler": "^7.2.0",
    "@angular/compiler-cli": "^7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/forms": "^7.2.0",
    "@angular/http": "^7.2.0",
    "@angular/platform-browser": "^7.2.0",
    "@angular/platform-browser-dynamic": "^7.2.0",
    "@angular/router": "^7.2.0",
    "ack-angular": "^1.10.0",
    "ack-angular-fx": "^2.3.1",
    "ack-css-boot": "^1.2.55",
    "ack-pug-bundler": "^1.3.15",
    "ack-reload": "^2.0.12",
    "ack-sass": "^1.1.1",
    "localforage": "^1.7.3",
    "nodedump": "^0.0.8",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "typescript": "3.1.1",
    "zone.js": "^0.8.26"
  }
}
