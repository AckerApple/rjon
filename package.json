{
  "name": "rjon",
  "version": "0.0.5",
  "description": "route js object notation is a way to standardize route metadata for use and testing in  universal route interpreter software",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "ack-webpack": "ack-webpack",
    "install:js": "ack-webpack install --out",
    "start": "ack-webpack reload www",
    "build": "npm run build:dist && npm run compile:aot && npm run build:js && npm run build:index && npm run build:css",
    "build:js": "ack-webpack app/index.aot.ts app/www/index.js --production --project app/tsconfig.json",
    "build:js:jit": "ack-webpack app/index.ts app/www/index.js --production",
    "build:index": "pug app/index.pug --out app/www/",
    "compile:aot": "ncp ./dist app/src/rjon && ngc --project app/tsconfig.aot.json",
    "compile:prefx": "ack-angular-fx --igniter void --select absoluteSwap,400,500 --effects fade,slide --out ./src/prefx.ts",
    "compile:templates": "npm run compile:templates:src && npm run compile:templates:example",
    "compile:templates:example": "ack-pug-bundler app/src/pugs/ app/src/templates/ --outType ts --oneToOne",
    "compile:templates:src": "ack-pug-bundler src/pugs/ src/templates/ --outType ts --oneToOne",
    "watch:templates": "npm run compile:templates -- --watch",
    "watch:jit": "ack-webpack app/index.ts app/www/index.js --watch --browser=app/www/ --project app/tsconfig.json",
    "build:css": "ack-sass app/styles.scss app/www/styles.css --production",
    "watch:css": "ack-sass app/styles.scss app/www/styles.css --watch",
    "watch:dist": "watch 'npm run build:dist && npm run compile:aot' ./src/ --ignoreDirectoryPattern=/templates/",
    "watch": "npm run watch:templates & npm run watch:dist & npm run watch:jit & npm run watch:css",
    "build:dist": "ngc --declaration --project src"
  },
  "keywords": [
    "rjon",
    "router",
    "routes",
    "route-map"
  ],
  "author": "Acker Apple",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AckerApple/rjon.git"
  },
  "bugs": {
    "url": "https://github.com/AckerApple/rjon/issues"
  },
  "homepage": "https://github.com/AckerApple/rjon#readme",
  "devDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "ack-angular": "^0.4.0",
    "ack-angular-fx": "^1.1.2",
    "ack-css-boot": "^1.2.29",
    "ack-pug-bundler": "^1.3.0",
    "ack-sass": "^1.1.0",
    "ack-webpack": "^1.1.21",
    "ng2-page-scroll": "^4.0.0-beta.6",
    "nodedump": "0.0.8",
    "pug": "^2.0.0-beta11",
    "pug-cli": "^1.0.0-alpha6",
    "rxjs": "^5.1.1",
    "ts-loader": "^2.0.2",
    "typescript": "^2.2.1",
    "webpack": "^2.3.1",
    "zone.js": "^0.7.8"
  },
  "jsDependencies": {
    "nodedump": "0.0.8",
    "ack-angular": "^0.4.0",
    "@angular/forms": "4.0.0",
    "@angular/router": "4.0.0"
  }
}
